{
    "name": "signpostmarv/daft-interface-collector",
    "description": "interface collector. daft reasons again.",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "SignpostMarv",
            "email": "me@signpostmarv.name"
        }
    ],
    "autoload": {
        "psr-4": {
            "SignpostMarv\\DaftInterfaceCollector\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SignpostMarv\\DaftInterfaceCollector\\Tests\\": "./tests/",
            "SignpostMarv\\DaftRouter\\Tests\\": "./vendor/signpostmarv/daft-router/tests/daft-router/"
        }
    },
    "config": {
        "classmap-authoritative": true,
        "sort-packages": true
    },
    "scripts": {
        "tests": [
            "phpunit -vvvv",
            "phpstan analyse -c ./phpstan.neon .php_cs.dist src tests",
            "psalm",
            "phpcpd src tests",
            "phpmnd src",
            "phpmnd tests",
            "php-cs-fixer fix --verbose --allow-risky=yes --dry-run"
        ]
    },
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.9.2",
        "phpstan/phpstan-phpunit": "^0.9.4",
        "phpstan/phpstan-strict-rules": "^0.9.0",
        "phpunit/phpunit": "^7.1",
        "povils/phpmnd": "^2.0",
        "roave/security-advisories": "dev-master",
        "sebastian/phpcpd": "^4.0",
        "signpostmarv/daft-router": "^0.1.0",
        "signpostmarv/php-cs-fixer-config": "^0.5.0",
        "vimeo/psalm": "^1.1"
    }
}
