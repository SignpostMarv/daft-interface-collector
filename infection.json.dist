{
    "timeout": 10,
    "source": {
        "directories": [
            "src"
        ]
    },
    "logs": {
        "text": "infection.log"
    },
    "mutators": {
        "@default": true,
        "Break_": {
            "ignore": [
                "SignpostMarv\\DaftInterfaceCollector\\StaticMethodCollector::CollectInterfacesFromImplementationCheckInterfaces"
            ]
        },
        "Continue_": {
            "ignore": [
                "SignpostMarv\\DaftInterfaceCollector\\StaticMethodCollector::CollectInterfacesFromImplementationTypes"
            ]
        },
        "Foreach_": {
            "ignore": [
                "SignpostMarv\\DaftInterfaceCollector\\StaticMethodCollector::CollectInterfacesFromImplementationCheckInterfaces"
            ]
        },
        "GreaterThan": {
            "ignore": [
                "SignpostMarv\\DaftInterfaceCollector\\StaticMethodCollector::FilterNonZeroArray"
            ]
        },
        "LogicalAnd": {
            "ignore": [
                "SignpostMarv\\DaftInterfaceCollector\\StaticMethodCollector::CollectInterfaces",
                "SignpostMarv\\DaftInterfaceCollector\\StaticMethodCollector::FilterReflectionMethod",
                "SignpostMarv\\DaftInterfaceCollector\\StaticMethodCollector::MakeMethodFilter"
            ]
        },
        "ProtectedVisibility": {
            "ignore": [
                "SignpostMarv\\DaftInterfaceCollector\\StaticMethodCollector::CollectInterfaces",
                "SignpostMarv\\DaftInterfaceCollector\\StaticMethodCollector::CollectInterfacesFromImplementation",
                "SignpostMarv\\DaftInterfaceCollector\\StaticMethodCollector::CollectInterfacesFromImplementationCheckInterfaces",
                "SignpostMarv\\DaftInterfaceCollector\\StaticMethodCollector::CollectInterfacesFromImplementationTypes",
                "SignpostMarv\\DaftInterfaceCollector\\StaticMethodCollector::FilterIsA",
                "SignpostMarv\\DaftInterfaceCollector\\StaticMethodCollector::FilterArrayOfInterfaces",
                "SignpostMarv\\DaftInterfaceCollector\\StaticMethodCollector::FilterArrayOfInterfaceOffsets",
                "SignpostMarv\\DaftInterfaceCollector\\StaticMethodCollector::FilterArrayOfInterfacesOrClasses",
                "SignpostMarv\\DaftInterfaceCollector\\StaticMethodCollector::FilterMethods",
                "SignpostMarv\\DaftInterfaceCollector\\StaticMethodCollector::FilterReflectionMethod",
                "SignpostMarv\\DaftInterfaceCollector\\StaticMethodCollector::FilterReflectionReturnType",
                "SignpostMarv\\DaftInterfaceCollector\\StaticMethodCollector::FilterNonZeroArray",
                "SignpostMarv\\DaftInterfaceCollector\\StaticMethodCollector::IsStringA",
                "SignpostMarv\\DaftInterfaceCollector\\StaticMethodCollector::IsStringInArray",
                "SignpostMarv\\DaftInterfaceCollector\\StaticMethodCollector::MakeMethodFilter"
            ]
        },
        "TrueValue": {
            "ignore": [
                "SignpostMarv\\DaftInterfaceCollector\\StaticMethodCollector::IsStringInArray"
            ]
        }
    }
}
